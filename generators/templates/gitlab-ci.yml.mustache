


stages:
  - build

{{#services}}
{{name}}:
    tags:
        - docker-build
    stage: build
    script:
        - docker login $CI_REGISTRY -u $CI_REGISTRY_USER -p $CI_REGISTRY_PASSWORD
        - docker build -t {{image}} {{build}}
        - docker push {{image}}
    after_script:
        - docker image rm {{image}}
{{/services}}


